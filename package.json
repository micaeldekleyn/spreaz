{
  "name": "spreaz",
  "version": "1.5.7",
  "description": "Spreaz is UI design language with React implementation.",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "prepack": "npm run prepack:babel && npm run prepack:flow",
    "prepack:babel": "node_modules/.bin/babel src --out-dir lib",
    "prepack:flow": "flow-copy-source src lib",
    "build:lib": "babel --out-dir lib src --copy-files",
    "build:dist": "rollup -c",
    "build": "npm run prepack && npm run build:lib && npm run build:dist",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "lodash": "^4.17.13",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "@storybook/addon-a11y": "^5.1.9",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-backgrounds": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addon-viewport": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.0",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-airbnb": "^3.2.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "eslint-watch": "^5.1.2",
    "flow-bin": "^0.100.0",
    "flow-copy-source": "^2.0.7",
    "prettier": "^1.17.1",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-img": "^1.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.2",
    "rollup-plugin-visualizer": "^2.5.0",
    "styled-components": "^4.3.2"
  },
  "peerDependencies": {
    "styled-components": "^4.3.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
